	"rules": {
		"connectionRules": [
			{
				"role":"epcEdge",
				"connects": [
					{
						"from":"event",
						"to":["function", "connector", "interface" ]
					},
					{
						"from":"function",
						"to":["event", "connector", "interface" ]
					},
					{
						"from":"connector",
						"to":["event", "function", "connector"]
					},
					{
						"from":"interface",
						"to":["function", "event"]
					}
				]
			},
			{
				"role":"relationEdge",
				"connects": [
					{
						"from":"event",
						"to":["InRelWithE"]
					},
					{
						"from":"function",
						"to":["InRelWithF"]
					},
