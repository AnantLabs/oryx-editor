	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Activity Looping.isMarker">true</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Activity Looping.key">looptype</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Activity Looping.value">Standard</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Multiple Instances.isMarker">true</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Multiple Instances.key">looptype</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Multiple Instances.value">Parallel</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Transaction.isMarker">false</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Transaction.key">callacitivity</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Transaction.value">true</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Compensation.isMarker">true</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskProperties.Compensation.key">isforcompensation</entry>
