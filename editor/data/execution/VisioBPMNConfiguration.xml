	<entry key="stencilsets.BPTBPMN11.config.taskToSubprocessThresholdWidth">2.1</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskToSubprocessThresholdHeight">1.7</entry>
	<entry key="stencilsets.BPTBPMN11.config.containerShouldHaveAtLeastXTimesTheSizeThreshold">1.5</entry>
	<entry key="stencilsets.BPTBPMN11.config.Unknown_NameU_But_With_Label_is">Description</entry> 
	<entry key="stencilsets.BPTBPMN11.config.Unknown_NameU_And_Without_Label_is">Flow</entry> 
	<entry key="stencilsets.BPTBPMN11.config.Skip_unknown_NameU_But_With_Label">false</entry>
	<entry key="stencilsets.BPTBPMN11.config.Skip_unknown_NameU_And_Without_Label">false</entry>
	
	<!--
		Mapping problems to tackle!!: 
		- In this visio stencil a pool is assembled from two elements: 
		body and head...but there's only a pool in BPMN 
		- Also: A Lane isn't distinct to a pool body... 
		- Conditional Flow is Sequence Flow with a specific attribute... 
		- Branching Condition is no element in BPMN 
	-->
	
	
	<!-- 
		ORYX SPECIFIC CONFIGURATION
	 -->
	<entry key="oryx.config.BPMN.resizableElements">Group,CollapsedPool,Pool,Lane,EventSubprocess,CollapsedEventSubprocess,Subprocess,CollapsedSubprocess,Task,TextAnnotation,DataObject</entry>	
	<entry key="oryx.config.BPMN.Group.minWidth">60</entry>
	<entry key="oryx.config.BPMN.Group.minHeight">50</entry>
	<entry key="oryx.config.BPMN.CollapsedPool.minWidth">20</entry>
	<entry key="oryx.config.BPMN.CollapsedPool.minHeight">20</entry>
	<entry key="oryx.config.BPMN.Pool.minWidth">20</entry>
	<entry key="oryx.config.BPMN.Pool.minHeight">20</entry>
	<entry key="oryx.config.BPMN.Lane.minWidth">10</entry>
	<entry key="oryx.config.BPMN.Lane.minHeight">20</entry>
	<entry key="oryx.config.BPMN.EventSubprocess.minWidth">120</entry>
