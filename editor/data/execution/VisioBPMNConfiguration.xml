	<!-- HEURISTICS -->
	
	<entry key="heuristics.Default_Page_Width">20.0</entry>
	<entry key="heuristics.Default_Page_Height">20.0</entry>
	<entry key="heuristics.Unit_To_Pixel_Exchange">100</entry>
	
	<!-- Sequenceflow: For a start or end of an edge all shapes within this distance will be found
	and the smallest will be assigned. So that a shape will be preferred to some swimlane.-->
	<entry key="heuristics.maxEdgeToShapeDistance">0.05</entry> <!-- Visio unit -->
	
	<!-- Interpretation of label-only shapes -->
	<entry key="heuristics.labelOnlyInterpretationMode">heuristic</entry> <!-- simple / heuristic --> <!-- for simple config see above -->
	<entry key="heuristics.labelOnlyIsLabelForAnotherShapeThreshold">0.5</entry>
	<entry key="heuristics.labelOnlyIsAnnotationToAnotherShapeThreshold">1.5</entry>
	<entry key="heuristics.labelOnlyIsDanglingAnnotation">true</entry>
	<entry key="heuristics.labelOnlyAnnotationType">Description</entry> <!-- Visio nameU -->
	<entry key="heuristics.labelOnlyAnnotationAssociation">Undirected Association</entry> <!-- Visio nameU -->
	<entry key="heuristics.labelOnlyTooLongForAnotherShapesLabelThreshold">30</entry>
	<!-- For simple interpretation mode of label-only shapes: set to another visio stencil nameU (e.g. Description) -->
	<entry key="heuristics.skipUnknownNameUButWithLabel">false</entry>
	<entry key="heuristics.unknownNameUButWithLabelType">Description</entry> 
	
	<!-- Interpretation of unnamed and empty shapes -->
	<entry key="heuristics.skipUnknownNameUAndWithoutLabel">false</entry>
	<entry key="heuristics.unknownNameUAndWithoutLabelType">Flow</entry> 
	
	<!-- Huge tasks are subprocesses? -->
	<entry key="stencilsets.BPTBPMN11.config.interpreteHugeTasksAsSubprocesses">true</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskToSubprocessThresholdWidth">2.1</entry>
	<entry key="stencilsets.BPTBPMN11.config.taskToSubprocessThresholdHeight">1.7</entry>
