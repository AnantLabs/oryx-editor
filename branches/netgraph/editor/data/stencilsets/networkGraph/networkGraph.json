                        ]
                    }
                ]
            }
        ],
        "containmentRules": [
            {
                "role": "Diagram",
                "contains": [
                    "host",
                    "switch"
                ]
            },
            {
                "role": "interface",
                "contains": [
                    "rule",
                    "gateway"
                ]
            },
            {
                "role": "program",
                "contains": [
                    "user",
                    "vulnerability"
                ]
            },
            {
                "role": "host",
                "contains": [
                    "program",
                    "user",
                    "file",
                    "interface",
                    "vulnerability",
                    "forwarding"
                ]
            }
        ],
        "morphingRules": [
            {
                "role": "TransitionMorph",
                "baseMorphs": [
                    "Host"
                ]
            },
            {
                "role": "FlowMorph",
                "baseMorphs": [
                    "Arc"
                ]
            }
